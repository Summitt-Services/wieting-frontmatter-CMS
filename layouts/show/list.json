[
  {{ range $j, $_ := .Pages }}
    {{ $title := .Title }}
    {{ $url := .Permalink | absURL }}
    {{ range $k, $_ := .Params.occurrences }}  
      {{ if or $j $k }},{{ end }} 
      {
        "title": "{{ $title }}",
        "date": "{{ dateFormat "2006-01-02T15:04:00-0700" .date }}",
        "url": "{{ $url }}"
      }
    {{ end }}  
  {{ end }}
]
