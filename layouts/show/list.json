[
  {{ range $j, $_ := .Pages }}
    {{ if $j }},{{ end }} 

    {{ $title := .Title }}
    {{ $url := .Permalink | absURL }}

    {{ range $k, $_ := .Params.occurancelist.occurance }}  
      {{ if $k }},{{ end }} 
      {
        "title": "{{ $title }}",
        "date": "{{ dateFormat "2006-01-02T15:04:00" .date }}",
        "url": "{{ $url }}"
      }
    {{ end }}  
  {{ end }}
]
